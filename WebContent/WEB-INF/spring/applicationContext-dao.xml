<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
				xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	
	<!-- Capa de Acceso a Datos DAO -->
    <bean id="usuarioDao" class="pe.com.sol.security.dao.impl.UsuarioDAOImpl">
    	<property name="dataSource" ref="dataSource"/>
    </bean>
    
    <bean id="marcaDao" class="pe.com.sol.portal.web.dao.impl.MarcaProdDAOImpl">
    	<property name="dataSource" ref="dataSource"/>
    </bean>
    
    <bean id="parametroDao" class="pe.com.sol.portal.web.dao.impl.ParametroDAOImpl">
    	<property name="dataSource" ref="dataSource"/>
    </bean>
    
    <bean id="productoDao" class="pe.com.sol.portal.web.dao.impl.ProductoDAOImpl">
    	<property name="dataSource" ref="dataSource"/>
    </bean>
  
    
    <bean id="clienteDao" class="pe.com.sol.portal.web.dao.impl.ClienteDAOImpl">
    	<property name="dataSource" ref="dataSource"/>
    </bean>
    

                  	
</beans>